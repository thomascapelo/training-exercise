<div>
   <h1><%= @tree.name %></h1>
    <tr>
        <th scope="row">Address</th>
        <td><%= @tree.address %></td>
    </tr>
    <%= @tree.description %>
    <%= @tree.fruit_type %>
    <%= @tree.price_per_year %>
    <%= @tree.quantity_per_year %>
    <%= link_to "Edit", edit_tree_path(@tree) %>
  <p><%= link_to 'Delete', tree_path(@tree), method: :delete, data: { confirm: 'Are you sure?' } %></p>
</div>